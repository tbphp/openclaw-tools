{
  "name": "openclaw-daily-summary",
  "version": "0.1.0",
  "description": "Daily summary plugin for OpenClaw â€” weather forecast and FX rates via Telegram",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "skills",
    "openclaw.plugin.json"
