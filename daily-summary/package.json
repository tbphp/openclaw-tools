    ]
  },
  "scripts": {
    "build": "tsc --skipLibCheck",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "keywords": [
    "openclaw",
    "openclaw-plugin",
    "daily-summary",
    "weather",
    "fx",
    "telegram"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.0"
  }
}
